      for i = 1,#tbl do 
        if #ctls == #tbl then
          print("PropagateStrings #ctls == #tbl")
          for idx = 1,#tbl do 
            ctls[i].String = tbl[i]
            print("Propagating ",tbl[i])
          end
        elseif #ctls ~= #tbl and #tbl > 1 then       
          for index = 1,#tbl do 
            if ctls[index].String == "" then 
              ctls[index].String = tbl[index] 
            end
          end
        elseif #tbl == 1 then  
          ctls[1].String = tbl[1] 
        end
      end
    end

    function DisableEnableCtls(ctrls,action) -- Disable or enable controls in array
      print("DisableEnableCtls(ctrls,action)")
      if action == "enable" then 
        for i,v in ipairs(ctrls) do 
          v.IsDisabled = false
        end
      elseif action == "disable" then 
        for i,v in ipairs(ctrls) do 
          v.IsDisabled = true
        end
      end
    end

    function Send(message) -- Send message to M3Webex  Plugin via Notification
      print("Send(message)")
      if DebugTx then
        print("TX: " .. message)
      end
      Notifications.Publish("M3 Webex " .. ID .. "TX", {Event = "SendCommand", Command = tostring(message)})
    end
    
    function WriteToCore() -- Write XML table to core directory
      print("WriteToCore()")
      if DirectoryValidated then 
        LuaXML.save(XmlTbl,"media/RmCtlsXmls/ciscormctls.xml")
        print("Wrote file to Core - media/RmCtlsXmls/ciscormctls.xml")
        PrintDebug.NewLine("Status","Wrote file to Core - media/RmCtlsXmls/ciscormctls.xml")
      else 
        print("Unable to write to core")
      end 
    end 

    function DirectoryWrite() -- Validate or create directory in core
      if not System.IsEmulating then
        local isdir = dir.get(directory)
        if isdir == nil then 
          dir.create(directory)
          print("Directory Created: "..directory)
          if Controls.Decode.Legend ~= "Parse XML" then
            Controls.Decode.Legend = "Parse XML"
            Controls.Decode.Color = "#ff7c7c7c"
          end
          DirectoryValidated = true
          if Controls.EncodetoTable.String ~= "" then 
            WriteToCore()
          else 
            print("No data present to write")
            PrintDebug.NewLine("Status","No data present to write")
          end  
        else 
          print("Directory available: "..directory)
          PrintDebug.NewLine("Status","Directory available: "..directory)
          DirectoryValidated = true
          WriteToCore()
        end 
      else 
        print("You are emulating, Directory not available")
        PrintDebug.NewLine("Status","You are emulating, Directory not available")
        DirectoryValidated = false
      end
    end
    
