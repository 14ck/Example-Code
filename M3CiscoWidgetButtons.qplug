    Controls.Decode.EventHandler = function() -- Decode Button Handler
      ClearWidgetIds()
      ErrorCount = 0
      WidgetIdsbtns = {}
      PrintDebug.Start("Status","Parsing XML")
      DisableEnableCtls(Controls.Fb,"disable")
      DisableEnableCtls(Controls.Btn,"disable")
      DisableEnableCtls(Controls.WidgetId,"disable")
      if Controls.PageSelect.String == "All" then 
        findWidgetIds(XmlTbl)
      else 
        findWidgetIdsonPage(XmlTbl)
      end 
      Timer.CallAfter(function()
        WidgetIdsbtns = FindDuplicates(WidgetIdsbtns)
      end,PropagateWait)
      Timer.CallAfter(function()
        table.sort(WidgetIdsbtns,naturalLess)
      end,PropagateWait+3)
      Timer.CallAfter(function()
          chunkCache = {} 
          print("WidgetIdsbtns",rapidjson.encode(WidgetIdsbtns,{pretty = true, sort_keys = sort}))
          PropagateStrings(WidgetIdsbtns,Controls.WidgetId) 
      end,PropagateWait+5)
      Timer.CallAfter(function() 
        DisableEnableCtls(Controls.Fb,"enable")
        DisableEnableCtls(Controls.Btn,"enable")
        DisableEnableCtls(Controls.WidgetId,"enable")
        if ErrorCount == 0 then 
          PrintDebug.NewLine("Status","XMl Parsing Successful")
        else 
          PrintDebug.NewLine("Status","Partially Parsed XML with "..tostring(ErrorCount).." Errors")
        end
      end,PropagateWait + 6)  
      Timer.CallAfter(DirectoryWrite,PropagateWait + 8)
    end 

