    function findWidgetIds(node) -- Find all WidgetIds in XML
      print("findWidgetIds(node)")
      if type(node) == "table" and node:tag() == "Widget" then
        tempwidgetId = nil
        isButton = false
        isText = false 
        isGroupBtn = false
    
        for index, child in ipairs(node) do
          if type(child) == "table" then
            if child:tag() == "Type" and child[1] == "Button" then
              isButton = true
            elseif child:tag() == "Type" and child[1] == "Text" then
              isText = true 
            elseif child:tag() == "Type" and child[1] == "GroupButton" then
              isGroupBtn = true  
            elseif child:tag() == "WidgetId" then
              tempwidgetId = child[1]
            end
          end
        end
    
        if isButton == true and tempwidgetId then
          table.insert(WidgetIdsbtns, tempwidgetId)
          CleanTable(WidgetIdsbtns)
        elseif isText == true and tempwidgetId then 
          table.insert(WidgetIdstext, tempwidgetId)
          CleanTable(WidgetIdstext)
        elseif isGroupBtn == true and tempwidgetId then 
          table.insert(WidgetIdsgrpbtns, tempwidgetId)
          CleanTable(WidgetIdsgrpbtns)  
        end
    
      else
        if type(node) == "table" then
          for _, child in ipairs(node) do
            findWidgetIds(child)
          end
        end
      end
    end

    function findWidgetIdsonPage(node) -- Find WidgetIds on selected Page
      local selectedpage = Controls.PageSelect.String

      if type(node) == "table" and node:tag() == "Page" then
        -- Look for matching Page Name
        for _, child in ipairs(node) do
          if type(child) == "table" and child:tag() == "Name" and child[1] == selectedpage then
            -- Found the correct Page
            for _, row in ipairs(node) do
              if type(row) == "table" and row:tag() == "Row" then
                for _, widget in ipairs(row) do
                  if type(widget) == "table" and widget:tag() == "Widget" then
                    local wid, wtype
                    for _, wchild in ipairs(widget) do
                      if type(wchild) == "table" then
                        if wchild:tag() == "WidgetId" then
                          wid = wchild[1]
                        elseif wchild:tag() == "Type" then
                          wtype = wchild[1]
                        end
                      end
                    end
                    if wid and wtype then
                      if wtype == "Button" then
                        table.insert(WidgetIdsbtns, wid)
                        CleanTable(WidgetIdsbtns)
                      elseif wtype == "Text" then
                        table.insert(WidgetIdstext, wid)
                        CleanTable(WidgetIdstext)
                      elseif wtype == "GroupButton" then
                        table.insert(WidgetIdsgrpbtns, wid)
                        CleanTable(WidgetIdsgrpbtns) 
                      end
                    end
                  end
                end
              end
            end
          end
        end
      elseif type(node) == "table" then
        -- keep recursing until we find the right Page
        for _, child in ipairs(node) do
          findWidgetIdsonPage(child)
        end
      end
    end

