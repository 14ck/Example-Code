      print("Initialization()")
      local isdir = dir.get(directory)
      if Controls.EncodetoTable.String == "" then
        Controls.PageSelect.IsDisabled = true
        Controls.Decode.IsDisabled = true
      end
      if Controls.PageSelect.String ~= "" then 
        PrintDebug.Clear()
        XmlTbl = {}
        XmlTbl = LuaXML.eval(Controls.EncodetoTable.String)
        findPages(XmlTbl)
        if NumofWidgets < 80 then 
          table.insert(tblPageNames, "All")
        end
        Controls.PageSelect.Choices = tblPageNames
      else 
        tblPageNames = {}
        Controls.PageSelect.Choices = tblPageNames
        Controls.PageSelect.String = ""
      end 
      ErrorCount = 0
      if DebugPrintCtl.String ~= "" then
        PrintDebug.Clear()
      end
      if not System.IsEmulating and isdir == nil then 
        Controls.Decode.Color = "#ffdc2727"
        Controls.Decode.Legend = "Save XML"
      else 
        Controls.Decode.Color = "#ff7c7c7c"
        Controls.Decode.Legend = "Parse XML"
      end
      print("SystemID:",ID)
      for i,v in ipairs(Controls.WidgetId) do 
        WidgetIdsbtns[i] = v.String  
      end
      SaveBoolean(Controls.Fb,InitStates)
      funcDeselect(Controls.Fb)
      if NumofExcl > 1 then 
        for i,v in ipairs(Controls.WidgetIdExclude) do 
          tblExclusions[i] = v.String
        end
      end
      if Controls.EncodetoTable.String == "" then 
        PrintDebug.Start("Status","Paste the roomcontrols.xml contents\n into the field above then click 'Read XML'")
      end
      print("WidgetIds",rapidjson.encode(WidgetIdsbtns,{pretty = true, sort_keys = true}))
    end
    ---------Notifications---------
      NotificationHandler = function(name, data)
        if type(data) == "table" then
          if data.Event == "SSHResponse" then
            if string.find(data.Response,"*e UserInterface Extensions Widget Action WidgetId:") then 
              print("Widget",data.Response)
              EvaluateforId(data.Response)
            end 
            if string.find(data.Response,"*e UserInterface Extensions Widget Action Type:") then 
              print("State",data.Response)
              EvaluateforType(data.Response)
            end 
          elseif data.Event == "Connected" then
            print("Connected")
            if CodecConnect == false then 
              Timer.CallAfter(function()
                RecallBoolean(Controls.Fb,InitStates)
              end,1) 
              Timer.CallAfter(function() 
                for i,v in ipairs(Controls.Fb) do 
                  if v.Boolean then 
                    Send("xCommand UserInterface Extensions Widget SetValue WidgetId:"..WidgetIdsbtns[i].." Value: active")
                  end
                end 
              CodecConnect = true end,2)
              Timer.CallAfter(function() Send("xFeedback register /Event/UserInterface/Extensions/Widget") end,5)
            end 
          elseif data.Event == "Disconnected" then
            print("Disconnected") 
            CodecConnect = false
          end
        end
      end

    subID = Notifications.Subscribe("M3 Webex " .. ID .. "RX", NotificationHandler)

