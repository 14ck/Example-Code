    function splitChunks(str) -- Split string into chunks of numbers and non-numbers for natural sorting
      if chunkCache[str] then
        return chunkCache[str]
      end

      local chunks = {}
      local i, len = 1, #str

      while i <= len do
        local c = string.sub(str, i, i)
        if c:match("%d") then
          local start = i
          while i <= len and string.sub(str, i, i):match("%d") do
            i = i + 1
          end
          table.insert(chunks, tonumber(string.sub(str, start, i - 1)))
        else
          local start = i
          while i <= len and not string.sub(str, i, i):match("%d") do
            i = i + 1
          end
          table.insert(chunks, string.sub(str, start, i - 1))
        end
      end

      chunkCache[str] = chunks
      return chunks
    end

    function naturalLess(a, b) -- Natural order comparison for sorting
      local ac, bc = splitChunks(a), splitChunks(b)
      local i = 1
      while i <= #ac and i <= #bc do
        local av, bv = ac[i], bc[i]
        if type(av) == "number" and type(bv) == "number" then
          if av ~= bv then return av < bv end
        else
          if tostring(av) ~= tostring(bv) then
            return tostring(av) < tostring(bv)
          end
        end
        i = i + 1
      end
      return #ac < #bc
    end

